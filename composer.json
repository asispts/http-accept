{
    "name": "hnet/http-accept",
    "description": "PHP Parser to deal with HTTP Accept, Accept-Language, Accept-Encoding, and Accept-Charset headers",
    "keywords": [
        "http",
        "http accept",
        "Accept",
        "Accept-Language",
        "Accept-Encoding",
        "Accept-Charset"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Asis Pattisahusiwa",
            "email": "asispts@gmail.com"
        }
    ],
    "homepage": "https://github.com/asispts/http-accept",
    "support": {
        "issues": "https://github.com/asispts/http-accept/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "HttpAccept\\": "src"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "pattisahusiwa/coding-standard": "^0.3.0",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^8.5|^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
