{
    "name": "xynha/http-accept",
    "description": "PHP Parser for dealing with HTTP Accept, Accept-Language, Accept-Encoding, and Accept-Charset headers",
    "keywords": [],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Asis Pattisahusiwa",
            "email": "asis.pattisahusiwa@gmail.com"
        }
    ],
    "homepage": "https://github.com/pattisahusiwa/http-accept",
    "support": {
        "issues": "https://github.com/pattisahusiwa/http-accept/issues"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
